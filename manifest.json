{
  "manifest_version": 2,
  "name": "FFP",
  "description": "This extension disables javascript and cookies Fairfax's metropolitian news sites.",
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "permissions": ["contentSettings"],
  "version": "2.0",
  "background": {
    "persistent": false,
    "scripts": ["ffp.js"]
  }
}
